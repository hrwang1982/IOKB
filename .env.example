# Server
APP_NAME=SKB
APP_ENV=development
DEBUG=true
HOST=0.0.0.0
PORT=8000

# MySQL Database
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=skb
MYSQL_PASSWORD=your_password_here
MYSQL_DATABASE=skb

# Elasticsearch
ES_HOST=localhost
ES_PORT=9200
ES_USER=elastic
ES_PASSWORD=your_es_password_here
ES_INDEX_PREFIX=skb

# InfluxDB
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your_influxdb_token_here
INFLUXDB_ORG=skb
INFLUXDB_BUCKET=metrics

# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_ALERT_TOPIC=skb-alerts
KAFKA_PERFORMANCE_TOPIC=skb-performance
KAFKA_LOG_TOPIC=skb-logs
KAFKA_CONSUMER_GROUP=skb-consumer

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT Auth
JWT_SECRET_KEY=your_jwt_secret_key_here_change_in_production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# LLM Configuration
# provider: aliyun, openai, anthropic, baidu, local, custom
# deploy_mode: api (调用远程API), local (本地部署模型)
LLM_PROVIDER=aliyun
LLM_DEPLOY_MODE=api
LLM_MODEL_NAME=qwen-turbo
LLM_API_KEY=your_api_key_here
LLM_API_BASE=https://dashscope.aliyuncs.com/api/v1
LLM_LOCAL_URL=
LLM_MAX_TOKENS=4096
LLM_MAX_INPUT_TOKENS=8192
LLM_MAX_OUTPUT_TOKENS=4096
LLM_TEMPERATURE=0.7

# Embedding Configuration
# provider: aliyun, openai, local, custom
# deploy_mode: api (调用远程API), local (本地部署模型)
EMBEDDING_PROVIDER=aliyun
EMBEDDING_DEPLOY_MODE=api
EMBEDDING_MODEL_NAME=text-embedding-v3
EMBEDDING_API_KEY=your_api_key_here
EMBEDDING_API_BASE=
EMBEDDING_LOCAL_URL=
EMBEDDING_DIMENSION=1024

# Rerank Configuration
# provider: aliyun, local, custom
# deploy_mode: api (调用远程API), local (本地部署模型)
RERANK_PROVIDER=aliyun
RERANK_DEPLOY_MODE=api
RERANK_MODEL_NAME=gte-rerank
RERANK_API_KEY=your_api_key_here
RERANK_API_BASE=
RERANK_LOCAL_URL=

# OCR Configuration
# provider: paddleocr, paddleocr_vl, aliyun, baidu, local, custom
# deploy_mode: api (调用远程API), local (本地部署模型)
OCR_PROVIDER=paddleocr_vl
OCR_DEPLOY_MODE=local
OCR_MODEL_PATH=
OCR_API_KEY=
OCR_API_BASE=
OCR_LOCAL_URL=

# 多模态VL模型配置（图片理解/视频分析）
# provider: aliyun, openai, local, custom
# deploy_mode: api (调用远程API), local (本地部署模型)
# 阿里云模型: qwen-vl-max, qwen-vl-plus  OpenAI模型: gpt-4o
VL_PROVIDER=aliyun
VL_DEPLOY_MODE=api
VL_MODEL_NAME=qwen-vl-max
VL_API_KEY=
VL_API_BASE=
VL_LOCAL_URL=

# Socket Server
SOCKET_HOST=0.0.0.0
SOCKET_PORT=9000

# File Storage
STORAGE_TYPE=local
STORAGE_PATH=./storage

# LDAP (Optional)
LDAP_ENABLED=false
LDAP_SERVER=ldap://localhost:389
LDAP_BASE_DN=dc=example,dc=com
LDAP_USER_DN=cn=admin,dc=example,dc=com
LDAP_PASSWORD=

# SSO (Optional)
SSO_ENABLED=false
SSO_PROVIDER=oauth2
SSO_CLIENT_ID=
SSO_CLIENT_SECRET=
SSO_AUTHORIZE_URL=
SSO_TOKEN_URL=
SSO_USERINFO_URL=

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]
